'''
Given two strings s and t, return true if t is an anagram of s, and false otherwise.

Example 1:
Input: s = "anagram", t = "nagaram"
Output: true

Example 2:
Input: s = "rat", t = "car"
Output: false

Constraints:
1 <= s.length, t.length <= 5 * 104
s and t consist of lowercase English letters.
'''





class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        
        if len(s) != len(t):
            return False

        sDict = {}
        tDict = {}

        for i in range(len(s)):
            if sDict.get(s[i]):
                sDict[s[i]] += 1
            else:
                sDict[s[i]] = 1

            if tDict.get(t[i]):
                tDict[t[i]] += 1
            else:
                tDict[t[i]] = 1


        if sDict == tDict:
            return True
        else:
            return False

